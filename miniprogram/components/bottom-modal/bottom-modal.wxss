.container {
  position: absolute;
  top:0;
  bottom: 0;
  left: 0;
  right: 0;
  z-index: 99;
  background: rgba(0,0,0,0.5);
}

.panel {
  position: absolute;
  left:0;
  bottom:0;
  height:300rpx;
  width: 100%;
  background-color: #f8f8f8;
}

.cancel {
  z-index: 99;
  position: absolute; 
  padding: 20rpx;
  right: 10rpx;
}